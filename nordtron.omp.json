        },
        {
          "foreground": "#b48ead",
          "style": "plain",
          "template": "<#5e81ac>--[</>{{.Profile}}{{if .Region}}@{{.Region}}{{end}}<#5e81ac>]</>",
          "type": "aws"
        },
        {
          "foreground": "#b48ead",
          "style": "plain",
          "template": "<#5e81ac>--[</>{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<#5e81ac>]</>",
          "type": "kubectl"
        },
        {
          "foreground": "#d8dee9",
          "style": "plain",
          "template": "<#5e81ac>[</>\uf0e7<#5e81ac>]</>",
          "type": "root"
        },
        {
          "foreground": "#d8dee9",
          "style": "plain",
          "template": "<#5e81ac>[x</><#5e81ac>]</>",
          "type": "exit"
        },
        {
          "foreground": "#00C5C7",
          "properties": {
            "time_format": "3:04 PM"
