{
  "$schema": "C:/Users/ACERq/AppData/Local/Programs/oh-my-posh/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "00C5C7",
          "style": "plain",
          "template": "<#5e81ac>\u250f[</>{{ .UserName }}<#5e81ac>]</>",
          "type": "session"
        },
        {
          "foreground": "#b48ead",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "<#5e81ac>--[</>{{ .HEAD }}<#5e81ac>]</>",
